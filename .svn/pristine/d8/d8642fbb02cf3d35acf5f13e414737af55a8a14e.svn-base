#izquierda
{
        position:absolute;
        height:100%;
        width:50%;
        left:0%;
        background-color:White;   
}
#derecha
{
        position:absolute;
        right:0%;
        height:100%;
        width:50%;
        background: #d3dae1; /* Old browsers */
        background: -moz-linear-gradient(top,  #d3dae1 0%, #e5eaee 50%, #d1d8e0 96%); /* FF3.6+ */
        background: -webkit-gradient(linear, left top, left bottom, color-stop(0%,#d3dae1), color-stop(50%,#e5eaee), color-stop(96%,#d1d8e0)); /* Chrome,Safari4+ */
        background: -webkit-linear-gradient(top,  #d3dae1 0%,#e5eaee 50%,#d1d8e0 96%); /* Chrome10+,Safari5.1+ */
        background: -o-linear-gradient(top,  #d3dae1 0%,#e5eaee 50%,#d1d8e0 96%); /* Opera 11.10+ */
        background: -ms-linear-gradient(top,  #d3dae1 0%,#e5eaee 50%,#d1d8e0 96%); /* IE10+ */
        background: linear-gradient(to bottom,  #d3dae1 0%,#e5eaee 50%,#d1d8e0 96%); /* W3C */
        filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#d3dae1', endColorstr='#d1d8e0',GradientType=0 ); /* IE6-9 */
}
